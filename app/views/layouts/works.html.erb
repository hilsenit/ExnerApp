<!DOCTYPE html>
<html lang="en">
  <head>
		<%= render partial: 'shared/google_analytics' %>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= header_title_helper %></title>
    <%= render 'application/favicon' %>
    <meta name="keywords" content="<%= @seo_keywords %>" />
    <% if content_for? :head %>
      <%= yield :head  %>
    <% end %>
    <% if content_for? :custom_calls %>
      <%= yield :custom_calls %>
    <% end %>
    <%= stylesheet_link_tag    "application" %>
		<%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>

  <body>
    <div class="container no-padding-x">
      <%= render 'cookies_eu/consent_banner', link: '/information-om-cookies', target: "_blank" unless params[:action] == 'information' %>
      <%= alert_notice_helper %>
			<%= render partial: "admin/navbar" if user_signed_in? %>
      <%= render partial: "shared/masthead" %>
      <%= render partial: "shared/navbar", locals: {categories: @categories} %>

      <div class="container-yield no-padding-x <%= 'container-p-top' unless content_for?(:padding_top_container) %>">
        <%= yield %>
      </div>
      <%= render partial: "shared/footer" %>

    </div>

  </body>
</html>
